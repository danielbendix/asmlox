var a = 0;
var b = 0;
var c = 0;
var d = 0;
var sum = 0;

while (a < 100) {
    b = 0;
    while (b < 100) {
        c = 0; 
        while (c < 100) {
            d = 0;
            while (d < 100) {
                sum = sum + 1;
                d = d + 1;
            }
            c = c + 1;
        }
        b = b + 1;
    }
    a = a + 1;
}

print sum;
